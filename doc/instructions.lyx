\begin_layout Subsubsection
Density of air
\end_layout

\begin_layout Standard
The density of ambient (moist) air is determined using the ideal gas equation
 
\begin_inset CommandInset citation
LatexCommand cite
key "wiki-density-of-air"

\end_inset

.
 The effects of water vapor are consciously omitted from this derivation
 for the sake of simplicity; total deviation is expected to remain within
 uncertainty from other sources.
 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\rho(T,P)=\frac{1000P}{R_{d}(T+273.15)}\; where\;\begin{aligned}\left[\rho\right] & =\unitfrac{kg}{m^{3}}\\
\left[T\right] & =\:^{\circ}\textrm{C}\\
\left[P\right] & =\textrm{kPa}\\
\left[R_{d}\right] & =\unitfrac{J}{(kg\: K)}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\textrm{kg}}{\textrm{m}^{3}}=\left(\frac{\cancel{\text{kPa}}}{1}\right)\left(\frac{\text{kg\,}\cancel{\text{K}}}{\cancel{\text{J}}}\right)\left(\frac{1}{\cancel{K}}\right)\left(\frac{\cancel{\text{J}}}{\cancel{\text{Pa}}\,\textrm{m}^{3}}\right)\left(\frac{10^{3}\:\cancel{\text{Pa}}}{\cancel{\text{kPa}}}\right)
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Dynamic Viscosity of Air
\end_layout

\begin_layout Standard
The dynamic viscosity of air is calculated using 
\emph on
Sutherland's Formula
\emph default
 
\begin_inset CommandInset citation
LatexCommand cite
key "wiki-viscosity"

\end_inset

.
 Variations in air pressure are disregarded without affecting a significant
 impact on derived values.
 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mu=\frac{\lambda(T+273.15)^{3/2}}{(T+273.15+C)(10^{6})}\: where\:\begin{aligned}\left[\mu\right] & =\unitfrac{kg}{(m\, s)}\\
\left[T\right] & =\:^{\circ}\text{C}\\
\left[C\right] & =\text{K}\\
\left[\lambda\right] & =\text{\mu Pa}\,\text{s}\,\text{K}^{1/2}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\text{kg}}{\text{m\,\ s}}=\text{Pa}\cdot\textrm{s}=\frac{\cancel{\text{\mu Pa}}\,\text{s}\,\cancel{\text{K}^{1/2}}\,\cancel{\text{K}^{3/2}}}{\cancel{\text{K}}}\left(\frac{\text{Pa}}{10^{6}\,\cancel{\text{\mu Pa}}}\right)
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Reynolds Number
\begin_inset CommandInset label
LatexCommand label
name "sub:Reynolds-Number"

\end_inset


\end_layout

\begin_layout Standard
The Reynolds number 
\begin_inset CommandInset citation
LatexCommand cite
key "wiki-Re-num"

\end_inset

 is calculated using the measured volumetric flow rate, derived density
 and dynamic viscosity of air, and static some dimensional constants .
 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Re e=\frac{39.37\rho\mathbb{Q}D}{\mu A(60)(10^{3})}\: where\:\begin{aligned}\left[\mathbb{\Re}e\right] & =\textrm{(dimless)}\\
\left[\rho\right] & =\unitfrac{kg}{m^{3}}\\
\left[\mathbb{Q}\right] & =\unitfrac{L}{min}\\
\left[D\right] & =\textrm{in}\\
\left[\mu\right] & \unitfrac{kg}{(m\, s)}\\
\left[A\right] & =\textrm{in}^{2}
\end{aligned}
\]

\end_inset


\begin_inset Formula 
\[
1=\frac{\cancel{\textrm{kg}}\,\cancel{\textrm{L}}\,\cancel{\textrm{in}}\,(\cancel{\textrm{m}}\,\cancel{\textrm{s}})}{\cancel{\textrm{m}^{3}}\,\cancel{\textrm{min}}\,\cancel{\textrm{kg}}\,\cancel{\textrm{in}^{2}}}\cdot\frac{39.37\,\cancel{\textrm{in}}}{\cancel{\textrm{m}}}\cdot\frac{\cancel{\textrm{min}}}{60\,\cancel{\textrm{s}}}\cdot\frac{\cancel{\textrm{m}^{3}}}{10^{3}\,\cancel{\textrm{L}}}
\]

\end_inset


\end_layout

